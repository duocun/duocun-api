{
  "name": "Order",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "accountId": {
      "type": "number",
      "required": true
    },
    "merchantId": {
      "type": "number",
      "required": true
    },
    "status": {
      "type": "string",
      "required": true,
      "default": "new"
    },
    "address": {
      "type": "string",
      "required": true,
      "default": ""
    },
    "note": {
      "type": "string",
      "required": false,
      "default": ""
    },
    "delivered": {
      "type": "date",
      "defaultFn": "now"
    },
    "created": {
      "type": "date",
      "defaultFn": "now"
    },
    "modified": {
      "type": "date",
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {
    "account": {
      "type": "belongsTo",
      "model": "Account",
      "foreignKey": "accountId"
    },
    "restaurant": {
      "type": "belongsTo",
      "model": "Restaurant",
      "foreignKey": "merchantId"
    },
    "items": {
      "type": "hasMany",
      "model": "OrderItem",
      "foreignKey": "orderId"
    },
    "deliveryAddress": {
      "type": "hasOne",
      "model": "Address",
      "polymorphic": "entity"
    }
  },
  "acls": [],
  "methods": {}
}
