{
  "name": "OrderItem",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "price": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "quantity": {
      "type": "number",
      "required": true
    },
    "orderId": {
      "type": "number",
      "required": true
    },
    "productId": {
      "type": "number",
      "required": true
    },
    "created": {
      "type": "date",
      "defaultFn": "now"
    },
    "modified": {
      "type": "date",
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {
    "order": {
      "type": "belongsTo",
      "model": "Order",
      "foreignKey": "orderId"
    },
    "product": {
      "type": "belongsTo",
      "model": "Product",
      "foreignKey": "productId"
    }
  },
  "acls": [],
  "methods": {}
}
